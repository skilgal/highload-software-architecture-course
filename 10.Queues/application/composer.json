{
    "type": "project",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "fzaninotto/faker": "^1.5",
        "pda/pheanstalk": "^4.0",
        "php-di/php-di": "^6.0",
        "predis/predis": "^1.1",
        "selective/basepath": "^2.1",
        "slim/psr7": "^1",
        "slim/slim": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "process-timeout": 0,
        "sort-packages": true
    }
}
